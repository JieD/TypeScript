// Within static member functions and static member accessors, the type of this is
// the constructor function type.

struct C {
>C : C

    constructor(x: number) { }
>x : number

    static foo: number;
>foo : number

    static bar() {
>bar : () => typeof C

        // type of this is the constructor function type
        var t = this;
>t : typeof C
>this : typeof C

        return this;
>this : typeof C
    }
}

var t = C.bar();
>t : typeof C
>C.bar() : typeof C
>C.bar : () => typeof C
>C : typeof C
>bar : () => typeof C

// all ok
var r2 = t.foo + 1;
>r2 : number
>t.foo + 1 : number
>t.foo : number
>t : typeof C
>foo : number

var r3 = t.bar();
>r3 : typeof C
>t.bar() : typeof C
>t.bar : () => typeof C
>t : typeof C
>bar : () => typeof C

var r4 = new t(1);
>r4 : C
>new t(1) : C
>t : typeof C

