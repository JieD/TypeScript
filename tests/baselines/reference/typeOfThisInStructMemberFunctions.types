// doc 4.2
// In the body of a static member function declaration, the type of this is the constructor
// function type.
// ok
struct C {
>C : C

    foo() {
>foo : () => void

        var r = this;
>r : C
>this : C
    }

    static bar() {
>bar : () => void

        var r2 = this;
>r2 : typeof C
>this : typeof C
    }
}

